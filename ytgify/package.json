{
  "name": "ytgify",
  "version": "1.0.19",
  "description": "Turn your favorite YouTube moments into shareable GIFs.",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "webpack --config webpack.config.cjs --mode development --watch",
    "dev:local": "API_BASE_URL=http://localhost:3000/api/v1 webpack --config webpack.config.cjs --mode development --watch",
    "build": "webpack --config webpack.config.cjs --mode production",
    "build:local": "API_BASE_URL=http://localhost:3000/api/v1 webpack --config webpack.config.cjs --mode production",
    "build:production": "scripts/build-production.sh",
    "clean": "rimraf dist",
    "format": "prettier --write 'src/**/*.{ts,tsx,js,jsx,css}'",
    "format:check": "prettier --check 'src/**/*.{ts,tsx,js,jsx,css}'",
    "knip": "knip",
    "knip:production": "knip --production",
    "typecheck": "tsc --noEmit",
    "lint": "eslint 'src/**/*.ts' 'src/**/*.tsx'",
    "lint:fix": "eslint 'src/**/*.ts' 'src/**/*.tsx' --fix",
    "test": "jest --config tests/jest.config.cjs",
    "test:watch": "jest --config tests/jest.config.cjs --watch",
    "test:coverage": "jest --config tests/jest.config.cjs --coverage",
    "test:e2e": "HEADLESS=true playwright test --config tests/playwright-mock.config.ts",
    "test:e2e:fast": "HEADLESS=true playwright test --config tests/playwright-mock.config.ts --workers=4 --retries=0",
    "test:e2e:headless": "HEADLESS=true playwright test --config tests/playwright-mock.config.ts",
    "test:e2e:headed": "playwright test --config tests/playwright-mock.config.ts --headed",
    "test:e2e:debug": "playwright test --config tests/playwright-mock.config.ts --debug",
    "test:e2e:ui": "playwright test --config tests/playwright-mock.config.ts --ui",
    "test:e2e:backend": "playwright test --config tests/playwright-backend.config.ts",
    "test:e2e:backend:debug": "playwright test --config tests/playwright-backend.config.ts --debug",
    "test:e2e:upload": "playwright test --config tests/playwright-backend.config.ts",
    "generate:test-videos": "cd tests/e2e-mock/fixtures/videos && chmod +x generate-test-videos.sh && ./generate-test-videos.sh",
    "test:all": "npm run test && npm run test:e2e",
    "test:integration": "npm run test:e2e:backend",
    "validate:pre-push": "npm run lint && npm run build && npm run typecheck && npm run knip:production && npm test && npm run test:e2e",
    "prepare": "husky"
  },
  "devDependencies": {
    "@eslint/js": "^9.35.0",
    "@playwright/test": "^1.55.0",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@types/chrome": "^0.0.254",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.5",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "autoprefixer": "^10.4.16",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.8.1",
    "eslint": "^9.17.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.0.0",
    "form-data": "^4.0.5",
    "globals": "^16.4.0",
    "html-webpack-plugin": "^5.6.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "knip": "^5.66.0",
    "mini-css-extract-plugin": "^2.7.6",
    "postcss": "^8.4.32",
    "postcss-loader": "^7.3.3",
    "prettier": "^3.1.1",
    "rimraf": "^5.0.5",
    "style-loader": "^3.3.3",
    "tailwindcss": "^3.4.0",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.5.1",
    "typescript": "^5.3.3",
    "typescript-eslint": "^8.43.0",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "@react-oauth/google": "^0.12.2",
    "gifenc": "^1.0.3",
    "gifski-wasm": "^2.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
